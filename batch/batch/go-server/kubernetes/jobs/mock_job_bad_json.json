{
  "batch_id": 1,
  "apiVersion": "v1",
  "metadata": {
    "name": "nginx-mysql",
    "labels": {
      "name": "nginx-mysql"
    }
  },
  "spAAAAAAAc": {
    "restartPolicy": "never",
    "containers": [
      {
        "name": "foo",
        "image": "bar",
        "command": "something",
        "args": "some_arg",
        "ports": [
          {
            "hostPort": 85,
            "containerPort": 80,
            "protocol": "TCP"
          },
          {
            "hostPort": 95,
            "containerPort": 90,
            "protocol": "UDP"
          }
        ],
        "env": {
          "name": "MYSQL_ROOT_PASSWORD",
          "value": "password"
        },
        "resources": {
          "requests": {
            "memory": "64Mi",
            "cpu": "250m"
          },
          "limits": {
            "memory": "128Mi",
            "cpu": "500m"
          }
        }
      },
      {
        "name": "mysql",
        "image": "mysql",
        "ports": [
          {
            "hostPort": 3306,
            "containerPort": 3306
          }
        ]
      }
    ]
  }
}
